<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - intro</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/blog2.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/quilljs/quill.snow.css" />
    <link rel="stylesheet" href="~/lib/quilljs/quill.bubble.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
    <environment include="Development">
        <link href="~/lib/bootstrap/bootstrap.min.css" rel="stylesheet">
    </environment>
    <environment exclude="Development">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
            integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    </environment>
</head>

<body class="bg-light">

    @Html.Partial("_HeaderPartial")

    @RenderBody()
    <footer class="text-center text-white">
        <section class="mb-4">
            <a class="btn btn-outline-light m-1" href="https://www.instagram.com/b.azizbek06/" role="button"><i
                    class="bi bi-instagram"></i></a>
            <a class="btn btn-outline-light btn-floating m-1" href="https://t.me/boboev_azizbek" role="button"><i
                    class="bi bi-telegram"></i></a>
            @* <a class="btn btn-outline-light btn-floating m-1" href="#!" role="button"><i class="bi
                bi-twitter"></i></a> *@
            <a class="btn btn-outline-light btn-floating m-1" href="#!" role="button"><i class="bi bi-meta"></i></a>
        </section>
    </footer>


    @await RenderSectionAsync("Scripts", required: false)


    <script src="~/lib/quilljs/quill.min.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/lib/bootstrap/bootstrap.bundle.min.js"></script>

</body>

</html>
